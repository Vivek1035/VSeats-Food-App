<svg xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 400 400">
  <!-- Background -->
  <rect width="400" height="400" fill="#f5f5f5"/>
  
  <!-- Plate -->
  <ellipse cx="200" cy="250" rx="180" ry="50" fill="#e0e0e0"/>
  <ellipse cx="200" cy="250" rx="160" ry="40" fill="white"/>
  
  <!-- Tandoori Chicken Pieces -->
  <!-- Piece 1 -->
  <path d="M150 200 L180 180 L210 200 L200 240 L160 230 Z" fill="#D32F2F"/>
  <path d="M150 200 L180 180 L210 200 L200 240 L160 230 Z" fill="#B71C1C" opacity="0.3"/>
  
  <!-- Piece 2 -->
  <path d="M220 190 L250 180 L270 210 L250 240 L220 230 Z" fill="#D32F2F"/>
  <path d="M220 190 L250 180 L270 210 L250 240 L220 230 Z" fill="#B71C1C" opacity="0.3"/>
  
  <!-- Piece 3 -->
  <path d="M130 210 L160 200 L170 230 L150 250 L120 230 Z" fill="#D32F2F"/>
  <path d="M130 210 L160 200 L170 230 L150 250 L120 230 Z" fill="#B71C1C" opacity="0.3"/>
  
  <!-- Spice marks -->
  <g fill="#FF6F00" opacity="0.7">
    <circle cx="160" cy="210" r="2"/>
    <circle cx="170" cy="200" r="2"/>
    <circle cx="180" cy="220" r="2"/>
    <circle cx="190" cy="210" r="2"/>
    <circle cx="200" cy="220" r="2"/>
    <circle cx="230" cy="200" r="2"/>
    <circle cx="240" cy="210" r="2"/>
    <circle cx="250" cy="220" r="2"/>
    <circle cx="140" cy="220" r="2"/>
    <circle cx="150" cy="230" r="2"/>
  </g>
  
  <!-- Garnish -->
  <!-- Lemon wedge -->
  <path d="M280 230 L300 220 L310 240 L290 250 Z" fill="#FFEB3B"/>
  <path d="M280 230 L300 220 L310 240 L290 250 Z" fill="white" opacity="0.3"/>
  <line x1="290" y1="225" x2="300" y2="245" stroke="white" stroke-width="1"/>
  
  <!-- Onion rings -->
  <ellipse cx="100" cy="220" rx="15" ry="5" fill="#E0E0E0" opacity="0.7"/>
  <ellipse cx="100" cy="220" rx="10" ry="3" fill="white"/>
  
  <!-- Coriander leaves -->
  <g fill="#4CAF50">
    <ellipse cx="120" cy="190" rx="5" ry="10" transform="rotate(30 120 190)"/>
    <ellipse cx="130" cy="180" rx="5" ry="10" transform="rotate(60 130 180)"/>
    <ellipse cx="270" cy="190" rx="5" ry="10" transform="rotate(-30 270 190)"/>
    <ellipse cx="260" cy="180" rx="5" ry="10" transform="rotate(-60 260 180)"/>
  </g>
  
  <!-- Grill marks -->
  <line x1="160" y1="190" x2="170" y2="200" stroke="#5D4037" stroke-width="2"/>
  <line x1="180" y1="190" x2="190" y2="200" stroke="#5D4037" stroke-width="2"/>
  <line x1="230" y1="190" x2="240" y2="200" stroke="#5D4037" stroke-width="2"/>
  <line x1="250" y1="190" x2="260" y2="200" stroke="#5D4037" stroke-width="2"/>
  <line x1="140" y1="210" x2="150" y2="220" stroke="#5D4037" stroke-width="2"/>
</svg>